otelcol.receiver.jaeger "default" {
	protocols {
		grpc { }

		thrift_http { }

		thrift_binary {
			max_packet_size = "65kB"
		}

		thrift_compact {
			max_packet_size = "65kB"
		}
	}

	output {
		traces = [otelcol.exporter.otlp.default.input]
	}
}

otelcol.exporter.otlp "default" {
	client {
		endpoint = "database:4317"
	}
}
